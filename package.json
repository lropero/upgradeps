{
  "name": "upgradeps",
  "version": "2.0.6",
  "description": "Audit and upgrade all dependencies in package.json.",
  "keywords": [
    "audit",
    "dependencies",
    "modules",
    "packages",
    "semver",
    "update",
    "upgrade"
  ],
  "homepage": "https://github.com/lropero/upgradeps#readme",
  "bugs": "https://github.com/lropero/upgradeps/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lropero/upgradeps.git"
  },
  "license": "ISC",
  "author": "Luciano Ropero <lropero@gmail.com>",
  "type": "module",
  "exports": "./upgradeps.js",
  "bin": {
    "upgradeps": "./upgradeps.js"
  },
  "scripts": {
    "lint": "npx prettier@latest --write . && npx standard@latest --fix",
    "sort": "npx sort-package-json@latest"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "chalk-animation": "2.0.3",
    "commander": "11.0.0",
    "detect-indent": "7.0.1",
    "figures": "5.0.0",
    "javascript-time-ago": "2.5.9",
    "latest-semver": "4.0.0",
